Host *
 AddKeysToAgent yes
 ForwardAgent yes
 ServerAliveInterval 240
# UseKeychain yes
 IdentityFile ~/.ssh/id_rsa

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/github_rsa
  IdentitiesOnly yes

Host gitlab.com
  HostName gitlab.com
  IdentityFile ~/.ssh/gitlab_rsa
  IdentitiesOnly yes

Host cetus
  User cv
  HostName cetus.uberspace.de
  IdentityFile ~/.ssh/cv_uberspace

Host workstation
  User root
  HostName workstation.christophvoigt.com
  IdentityFile ~/.ssh/github_rsa
  Port 3222

Host horologium
  User jmb
  HostName horologium.uberspace.de
  IdentityFile ~/.ssh/jmb_uberspace
